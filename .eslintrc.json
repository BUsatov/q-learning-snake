{
  "extends": ["prettier", "prettier/flowtype"],
  "plugins": ["flowtype", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "quotes": ["error", "single"]
  }
}
